@article{poonswadtsuji1994i,
  title = {Ranges of Males of the {{Great Hornbill}} {{{\emph{Buceros}}}}{\emph{ Bicornis}} , {{Brown Hornbill}} {{{\emph{Ptilolaemus}}}}{\emph{ Tickelli}} and {{Wreathed Hornbill}} {{{\emph{Rhyticeros}}}}{\emph{ Undulatus}} in {{Khao Yai National Park}}, {{Thailand}}},
