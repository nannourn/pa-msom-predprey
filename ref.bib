@article{paulyetal2022sr,
  title = {High Deforestation Trajectories in {{Cambodia}} Slowly Transformed through Economic Land Concession Restrictions and Strategic Execution of {{REDD}}+ Protected Areas},
  author = {Pauly, Maren and Crosse, Will and Tosteson, Joshua},
  year = {2022},
