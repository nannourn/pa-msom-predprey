@article{gibbsetal2007erl,
  title = {Monitoring and Estimating Tropical Forest Carbon Stocks: Making {{REDD}} a Reality},
  shorttitle = {Monitoring and Estimating Tropical Forest Carbon Stocks},
  author = {Gibbs, Holly K and Brown, Sandra and Niles, John O and Foley, Jonathan A},
  year = {2007},
